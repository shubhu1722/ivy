{
	"ctype": "subProcess",
	"title": "STOP",
	"uuid": "assesmentUUID",
	"subMenuIndex": 0,
	"clientName": "CISCO",
	"openRightNav": false,
	"hooks": [
	  {
		"type": "microservice",
		"hookType": "beforeInit",
		"config": {
		  "microserviceId": "getSubProcessPageBody",
		  "requestMethod": "get",
		  "params": {
			"subProcessId": "#getReferenceDataKeys.subProcess.Cisco.STOP",
			"userName": "#userAccountInfo.PersonalDetails.USERID"
		  },
		  "isLocal": false,
		  "LocalService": "assets/Responses/SubProcess/CISCO/Hu2_StopSubProcess.json"
		},
		"responseDependents": {
		  "onSuccess": {
			"actions": [
			  {
				"type": "context",
				"config": {
				  "requestMethod": "add",
				  "key": "stopData",
				  "data": "responseData"
				}
			  },
			  {
				"type": "renderTemplate",
				"config": {
				  "mode": "remote",
				  "targetId": "mainPageContentBodysp",
				  "templateId": "stopData"
				}
			  }
			]
		  },
		  "onFailure": {
			"actions": [
			  {
				"type": "toast",
				"message": "Failed to fetch Cisco stop JSON"
			  }
			]
		  }
		}
	  }
	]
  }