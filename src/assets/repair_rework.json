{
    "ctype": "subProcess",
    "title": "REWORK",
    "uuid": "reworkUUID",
    "subMenuIndex": 4,
    "clientName": "HP",
    "openRightNav": false,
    "hooks": [
        {
            "type": "microservice",
            "hookType": "beforeInit",
            "config": {
                "microserviceId": "getSubProcessPageBody",
                "requestMethod": "get",
                "params": {
                    "subProcessId": "#wcSubprocess.REWORK.subProcessID",
                    "userName": "#userAccountInfo.PersonalDetails.USERID"
                },
                "isLocal": false,
                "LocalService": "assets/Responses/SubProcess/HP/reworkSubProcess.json"
            },
            "responseDependents": {
                "onSuccess": {
                    "actions": [
                        {
                            "type": "renderTemplate",
                            "config": {
                                "mode": "remote",
                                "targetId": "mainPageContentBodysp",
                                "templateId": "getSubProcessPageBody"
                            }
                        }
                    ]
                },
                "onFailure": {
                    "actions": [
                        {
                            "type": "toast",
                            "message": "Failed to fetch rework JSON"
                        }
                    ]
                }
            }
        }
    ]
}