{
    "ctype": "subProcess",
    "title": "Hold",
    "uuid": "SubprocessSectionUUID",
    "subMenuIndex": 0,
    "clientName": "VeriFone",
    "openRightNav": false,
    "hooks": [
        {
            "type": "microservice",
            "hookType": "beforeInit",
            "config": {
                "microserviceId": "getSubProcessPageBody",
                "requestMethod": "get",
                "params": {
                    "subProcessId": "#getReferenceDataKeys.subProcess.VERIFONE.HOLD",
                    "userName": "#userAccountInfo.PersonalDetails.USERID"
                },
                "isLocal": false,
                "LocalService": "assets/Responses/SubProcess/VERIFONE/verifoneHoldSubProcess.json"
            },
            "responseDependents": {
                "onSuccess": {
                    "actions": [
                        {
                            "type": "context",
                            "config": {
                                "requestMethod": "add",
                                "key": "TestData",
                                "data": "responseData"
                            }
                        },
                        {
                            "type": "renderTemplate",
                            "config": {
                                "mode": "remote",
                                "targetId": "mainPageContentBodysp",
                                "templateId": "TestData"
                            }
                        }
                    ]
                },
                "onFailure": {
                    "actions": [
                        {
                            "type": "toast",
                            "message": "Failed to fetch TEST JSON"
                        }
                    ]
                }
            }
        }        
    ]
}