{
    "ctype": "subProcess",
    "title": "ASSEMBLY",
    "uuid": "dashboardUUID",
    "clientName": "DELL",
    "subMenuIndex": 0,
    "openRightNav": false,
    "hooks": [
      {
        "type": "microservice",
        "hookType": "beforeInit",
        "config": {
          "microserviceId": "getSubProcessPageBody",
          "requestMethod": "get",
          "params": {
            "subProcessId": "#wcSubprocess.ASSEMBLY.subProcessID",
            "userName": "#userAccountInfo.PersonalDetails.USERID"
          },
          "isLocal": false,
          "LocalService": "assets/Responses/SubProcess/DELL/dellAssemblySubProcess.json"
        },
        "responseDependents": {
          "onSuccess": {
            "actions": [
              {
                "type": "renderTemplate",
                "config": {
                  "mode": "remote",
                  "targetId": "mainPageContentBodysp",
                  "templateId": "getSubProcessPageBody"
                }
              }
            ]
          },
          "onFailure": {
            "actions": [
              {
                "type": "toast",
                "message": "Failed to fetch Dell VMI JSON"
              }
            ]
          }
        }
      }
    ]
  }